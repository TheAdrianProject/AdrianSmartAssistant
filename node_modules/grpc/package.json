{
  "name": "grpc",
  "version": "1.0.1",
  "author": {
    "name": "Google Inc."
  },
  "description": "gRPC Library for Node",
  "homepage": "http://www.grpc.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grpc/grpc.git"
