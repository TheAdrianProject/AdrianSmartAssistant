    "third_party/zlib",
    "third_party/boringssl",
    "binding.gyp"
  ],
  "main": "src/node/index.js",
  "license": "BSD-3-Clause",
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "esnext": true,
    "freeze": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "maxlen": 80,
    "mocha": true,
    "newcap": true,
    "node": true,
    "noarg": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": "vars"
